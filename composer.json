{
    "name": "sahilchadha/cakephp-fyluploader",
    "description": "cakephp-fyluploader plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "cakephp-fyluploader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cakephp-fyluploader\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
     "extra": {
        "installer-name": "fylupload"
    }
}
